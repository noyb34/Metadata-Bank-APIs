      "pattern": "^(.*)$"
    },
    "Credited": {
      "type": "boolean"
    },
    "StartDate": {
      "type": "string",
      "format": "date"
    },
    "EndDate": {
      "type": "string",
      "format": "date"
    },
    "FundingType": {
      "type": "integer"
    },
    "ProducerFunding": {
      "type": "boolean"
    },
    "Funder": {
      "type": "string",
      "pattern": "^(.*)$"
    },
    "LogicalAssetIDList": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "LogicalAssetID": {
            "type": "string",
            "pattern": "^(.*)$"
          }
        },
        "required": [
          "LogicalAssetID"
        ]
      }
