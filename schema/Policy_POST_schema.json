{
  "type": "object",
  "required": [
    "Permission",
    "PolicyType",
    "ReferentType",
    "StructuralType",
    "Title"
  ],
  "properties": {
    "EntityType": {
      "type": "string",
      "pattern": "^(.*)$"
    },
    "PolicyType": {
      "type": "string",
      "pattern": "^(.*)$"
    },
    "ReferentType": {
      "type": "string",
      "pattern": "^(.*)$",
      "enum": [
        "Series",
        "Season",
        "TV",
        "Movie",
        "Short",
        "Web",
        "Compilation",
        "Supplemental",
        "Offer",
        "Policy"
      ]
    },
    "StructuralType": {
      "type": "string",
      "pattern": "^(.*)$",
      "enum": [
        "Abstraction",
        "Performance",
        "Digital"
      ]
    },
    "Title": {
      "type": "string",
      "pattern": "^(.*)$"
    },
    "LogicalAssetIDList": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "LogicalAssetID": {
            "type": "string",
            "pattern": "^(.*)$"
          }
        },
        "required": [
          "LogicalAssetID"
        ]
      }
    },
    "Permission": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Constraint": {
            "type": "object",
            "properties": {
              "PlayType": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "Action": {
                "type": "string",
                "pattern": "^(.*)$",
                "enum": [
                  "Television",
                  "Linear Streaming",
                  "Non-Commercial",
                  "Commercial",
                  "Broadcast",
                  "Pay",
                  "Pay Premium",
                  "Linear"
                ]
              },
              "MaximumExploitation": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "Calculated": {
                "type": "boolean"
              },
              "TermStartDate": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "TermEndDate": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "LogicalAssetIDList": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "LogicalAssetID": {
                      "type": "string",
                      "pattern": "^(.*)$"
                    }
                  },
                  "required": [
                    "LogicalAssetID"
                  ]
                }
              },
              "EntryList": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "EntryClass": {
                      "type": "string",
                      "pattern": "^(.*)$"
                    },
                    "EntryUID": {
                      "type": "string",
                      "pattern": "^(.*)$"
                    }
                  },
                  "required": [
                    "EntryClass",
                    "EntryUID"
                  ]
                }
              }
            },
            "required": [
              "Action",
              "Calculated",
              "MaximumExploitation",
              "TermStartDate"
            ]
          }
        },
        "required": [
          "Constraint"
        ]
      }
    }
  }
}