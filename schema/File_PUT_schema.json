{
    "type": "object",
    "properties": {
        "ParentUID": {
            "minLength": 1,
            "pattern": "^(.*)$",
            "type": "string"
        },
        "FileClass": {
            "transform": ["trim", "toEnumCase"],
            "type": "string",
            "enum": [
                "Manifest",
                "Metadata",
                "Media",
                "Avail",
                "Ancillary"
            ]
        },
        "FileName": {
            "minLength": 1,
            "pattern": "^(.*)$",
            "type": "string"
        },
        "FileDeliveryStatus": {
            "transform": ["trim", "toEnumCase"],
            "type": "string",
            "enum": [
                "Removed",
                "Delivered",
                "Future",
                "Other"
            ]
        },
        "AlternateAssetIDList": {
            "type": "array",
            "uniqueItems": true,
            "items": {
                "required": [
                    "AssetUID"
                ],
                "type": "object",
                "properties": {
                    "AssetUID": {
                        "pattern": "^(.*)$",
                        "type": "string"
                    }
                }
            }
        }
    }
}